{
  "expo": {
    "name": "kingdomTennis",
    "slug": "tennis-diary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smokiebacon.tennisdiary"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smokiebacon.tennisdiary"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5b976b0a-f2f2-4c61-be72-64dd089d15fd"
      },
    "plugins" : [
      "@react-native-google-signin/google-signin"
    ]
    },
    "owner": "smokiebacon"
  }
}
